{
  "name": "honey-chat",
  "version": "0.0.1",
  "author": "DmitryScaletta",
  "description": "Alternative twitch-like chat with history and some enhancements",
  "license": "MIT",
  "private": true,
  "homepage": "https://honeykingdom.github.io/chat",
  "dependencies": {
    "camel-case": "^3.0.0",
    "date-fns": "^2.6.0",
    "emojilib": "^2.4.0",
    "gh-pages": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scrollbars-custom": "^4.0.21",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.0-rc.1",
    "tekko": "^2.4.0",
    "twemoji-parser": "^12.1.0",
    "url-regex": "^5.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier",
      "react-hooks"
    ],
    "globals": {
      "fetch": true,
      "window": true,
      "document": true,
      "localStorage": true,
      "navigator": true,
      "WebSocket": true
    },
    "env": {
      "jest": true
    },
    "parser": "babel-eslint"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
